[paths]
output_dir = %POOL_PATH%
data_dir = /var/lib/oz
screenshot_dir = /var/lib/oz/screenshots
# sshprivkey = /etc/oz/id_rsa-icicle-gen

[libvirt]
uri = qemu:///system
image_type = qcow2
# type = kvm
bridge_name = %EXT_BRIDGE%
cpus = 2
memory = 4096

[cache]
original_media = yes
modified_media = no
jeos = no

[icicle]
safe_generation = no
